  <bootloader t="map">
    <global t="map">
      <append>load_ramdisk=1 console=ttyS0,9600n quiet security=apparmor mitigations=off</append>
      <cpu_mitigations>off</cpu_mitigations>
      <gfxmode>auto</gfxmode>
      <hiddenmenu>false</hiddenmenu>
      <os_prober>true</os_prober>
      <secure_boot>true</secure_boot>
      <serial>serial --unit=0 --speed=9600 --parity=no</serial>
      <terminal>gfxterm serial</terminal>
      <timeout t="integer">8</timeout>
      <trusted_grub>false</trusted_grub>
      <update_nvram>true</update_nvram>
      <xen_append>console=hvc0</xen_append>
      <xen_kernel_append>console=com1 com1=9600</xen_kernel_append>
    </global>
    <loader_type>grub2</loader_type>
  </bootloader>


  <firewall t="map">
    <default_zone>public</default_zone>
    <enable_firewall t="boolean">true</enable_firewall>
    <log_denied_packets>off</log_denied_packets>
    <start_firewall t="boolean">true</start_firewall>
  </firewall>


